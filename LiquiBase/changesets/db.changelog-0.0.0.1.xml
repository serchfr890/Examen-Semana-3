<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="sergio" id="20190303-01">
        <createTable tableName="logins">
            <column name="id" type="int" autoIncrement	="true">
                <constraints nullable="true" primaryKey="true" unique="false"/>
            </column>
            <column name="user_name" type="VARCHAR(10)">
                <constraints nullable="true" primaryKey="false" unique="false"/>
            </column>
            <column name="password" type="VARCHAR(10)">
                <constraints nullable="true" primaryKey="false" unique="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog> 